# Supabase Configuration (Required)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Gemini AI API Key (Required for AI Coach feature)
# IMPORTANT: This is a server-side only key - do NOT use NEXT_PUBLIC_ prefix
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=

# Upstash Redis (Optional - for rate limiting)
# Get these from: https://console.upstash.com/
# If not provided, rate limiting falls back to in-memory (not recommended for production)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# CSRF Protection Secret (Required for production)
# Generate with: openssl rand -base64 32
CSRF_SECRET=

# Feature flags
NEXT_PUBLIC_USE_GLOBAL_STATS=true

# Optional: Vercel analytics auto-detected
# NEXT_PUBLIC_VERCEL_ANALYTICS_ID=
